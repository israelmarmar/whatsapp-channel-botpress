{
  "name": "whatsapp-channel-botpress",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "scripts": {
    "build": "tsc --build",
    "start": "node dist/index.js",
    "watch": "tsc --build --watch",
    "dev": "ts-node-dev src/index.ts",
    "dockerBuild": "sudo docker run -v \"$PWD:/botpress/modules/custom_module\" --rm ghcr.io/botpress/botpress/module-builder:0.0.3 sh -c 'cd /botpress/modules/custom_module && yarn && yarn buildv2 && yarn package'",
    "buildv2": "node ../../build/module-builder/bin/entry build",
    "watchv2": "node ../../build/module-builder/bin/entry watch",
    "package": "node ../../build/module-builder/bin/entry package"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@botpress/messaging-client": "^1.2.0",
    "axios": "^0.27.2",
    "botpress": "^10.51.10",
    "lodash": "^4.17.21"
  }
}
